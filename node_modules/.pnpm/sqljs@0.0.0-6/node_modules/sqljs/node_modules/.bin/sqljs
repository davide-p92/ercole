#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/davide/Downloads/proj_javescript/ercole/node_modules/.pnpm/sqljs@0.0.0-6/node_modules/sqljs/bin/node_modules:/home/davide/Downloads/proj_javescript/ercole/node_modules/.pnpm/sqljs@0.0.0-6/node_modules/sqljs/node_modules:/home/davide/Downloads/proj_javescript/ercole/node_modules/.pnpm/sqljs@0.0.0-6/node_modules:/home/davide/Downloads/proj_javescript/ercole/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/davide/Downloads/proj_javescript/ercole/node_modules/.pnpm/sqljs@0.0.0-6/node_modules/sqljs/bin/node_modules:/home/davide/Downloads/proj_javescript/ercole/node_modules/.pnpm/sqljs@0.0.0-6/node_modules/sqljs/node_modules:/home/davide/Downloads/proj_javescript/ercole/node_modules/.pnpm/sqljs@0.0.0-6/node_modules:/home/davide/Downloads/proj_javescript/ercole/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/sqljs" "$@"
else
  exec node  "$basedir/../../bin/sqljs" "$@"
fi
